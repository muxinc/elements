{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "i18n": {},
    "lint": {},
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**/*"],
      "dependsOn": ["^build"]
    },
    "build:packages": {
      "outputs": ["dist/**/*"],
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**/*", "build/**/*", ".next/**/*", ".svelte-kit/**/*"],
      "dependsOn": ["^build"]
    }
  }
}
