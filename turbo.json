{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "i18n": {},
    "clean": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/**/*"],
      "dependsOn": ["^copypolyfills", "copypolyfills", "^build"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["build:types", "^build"]
    },
    "copypolyfills": {
      "outputs": ["src/polyfills/**/*"],
      "dependsOn": ["^copypolyfills"]
    },
    "build:types": {
      "outputs": ["dist/types/**/*", "dist/types-ts3.4/**/*"],
      "dependsOn": ["^build:types"]
    },
    "build:packages": {
      "outputs": ["dist/**/*"],
      "dependsOn": ["^build"]
    },
    "build": {
      "outputs": ["dist/**/*", "build/**/*", ".next/**/*", ".svelte-kit/**/*", ".vercel/**/*"],
      "dependsOn": ["^copypolyfills", "copypolyfills", "build:types", "^build"]
    }
  }
}
