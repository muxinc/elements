{
  "name": "demo-vanilla-ts-esm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "author": "Mux, Inc.",
  "license": "MIT",
  "scripts": {
    "dev": "yarn esbuild --servedir=public",
    "esbuild": "esbuild src/** --bundle --target=es2019 --format=esm --outdir=public/dist",
    "build": "yarn esbuild --minify",
    "predeploy": "yarn run build",
    "deploy": "gh-pages --dist public --dest ./examples/mux-elements/"
  },
  "devDependencies": {
    "esbuild": "^0.14.2",
    "gh-pages": "^3.2.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@mux-elements/mux-audio": "^0.2.0",
    "@mux-elements/mux-player": "^0.1.0-alpha.3",
    "@mux-elements/mux-video": "^0.2.0"
  }
}
